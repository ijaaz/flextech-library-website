.icon
  text-decoration none
  border-bottom none
  position relative
  &:before
    -moz-osx-font-smoothing grayscale
    -webkit-font-smoothing antialiased
    font-family FontAwesome
    font-style normal
    font-weight normal
    text-transform none !important
  & > .label
    display none
  &.rounded
    background-color #383b43
    border-radius 100%
    color #fff
    display inline-block
    height 3.25em
    line-height 3.25em
    text-align center
    width 3.25em
    &.big
      font-size 2.5em
      margin-bottom 0.5em
    &.color1
      background-color #3cadd4
    &.color2
      background-color #3c61d4
    &.color3
      background-color #633cd4
    &.color4
      background-color #af3cd4
    &.color5
      background-color #d43cad
    &.color6
      background-color #d43c61
    &.color7
      background-color #d4633c
    &.color8
      background-color #d4af3c
    &.color9
      background-color #add43c
    &.color10
      background-color #61d43c
    &.fa-twitter
      background-color #2daae4
    &.fa-facebook
      background-color #39599f
    &.fa-google-plus
      background-color #c91e1f
    &.fa-dribbble
      background-color #c4376b
